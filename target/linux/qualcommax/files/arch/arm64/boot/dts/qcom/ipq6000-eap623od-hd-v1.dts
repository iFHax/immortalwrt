/dts-v1/;

#include "ipq6000-tplink.dtsi"

/ {
	model = "TP-Link EAP623-Outdoor HD V1.0";
	compatible = "tplink,eap623od-hd-v1", "qcom,ipq6018";

	leds {
		compatible = "gpio-leds";

		led_sys_wlan: sys-wlan {
			color = <LED_COLOR_ID_YELLOW>;
			function = LED_FUNCTION_WLAN;
			gpios = <&tlmm 32 GPIO_ACTIVE_HIGH>;
		};
	};
};

&tlmm {
	phy_pins: phy-reset-pin {
		pins = "gpio77";
		function = "gpio";
		bias-pull-up;
	};
};

&qpic_nand {
	status = "okay";

	nand@0 {
		reg = <0>;
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "qcom,smem-part";
		};
	};
};

&wifi {
	status = "okay";
	qcom,ath11k-fw-memory-mode = <1>;
	qcom,ath11k-calibration-variant = "TPLink-EAP623-Outdoor-HD-v1";
};
